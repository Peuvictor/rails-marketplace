<%= simple_form_for(shirt) do |f| %>
  <%= f.input :name %>
  <%= f.input :country %>
  <%= f.input :team %>
  <%= f.input :size %>
  <%= f.input :price %>
  <%= f.input :year %>
  <%= f.input :description %>
  <%= f.input :color %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
